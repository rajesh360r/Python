tuple_1 = ("Rajesh", "Suraj", "Rakesh", "Abhi", "Amit", "Sumit")
tuple_2 = (1234, 5678, 9012, 3456, 7890, 4321)


user_name = input("Enter user name: ")
user_password = int(input("Enter  password: "))

if user_name in tuple_1:
    index = tuple_1.index(user_name)
    if tuple_2[index] == user_password:
        print("Login successful! Welcome,", user_name)
    else:
        print("Incorrect password.")
else:
    print("Username not found.")
  
